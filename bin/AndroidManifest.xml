<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.equuleus.equuleuscapstone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.equuleus.equuleuscapstone.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.equuleus.equuleuscapstone.MainScreen"
            android:label="@string/title_activity_main_screen" >
        </activity>
        <activity
            android:name="com.equuleus.equuleuscapstone.ContactsScreen"
            android:label="@string/title_activity_contacts_screen" >
        </activity>
        <activity
            android:name="com.equuleus.equuleuscapstone.TimesScreen"
            android:label="@string/title_activity_times_screen" >
        </activity>
        <activity
            android:name="com.equuleus.equuleuscapstone.NotificationScreen"
            android:label="@string/title_activity_notification_screen" >
        </activity>
        <activity
            android:name="com.equuleus.equuleuscapstone.NewEventScreen"
            android:label="@string/title_activity_new_event_screen" >
        </activity>
    </application>

</manifest>
